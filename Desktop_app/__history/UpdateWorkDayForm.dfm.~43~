  object UniSQLUpdateWorkday: TUniSQL
    Connection = DM.UniXBill
    SQL.Strings = (
      'update calendar c'
      'set c_is_work_day_locally=false, '
      'c_reason_dayoff_locally = :p_reason, '
      'c_log_changes = :p_log '
      'where calendar_id = :p_calendar_id')
    Left = 430
    Top = 240
    ParamData = <
      item
        DataType = ftString
        Name = 'p_reason'
        ParamType = ptInput
        Value = nil
      end
      item
        DataType = ftString
        Name = 'p_log'
        ParamType = ptInput
        Value = nil
      end
      item
        DataType = ftInteger
        Name = 'p_calendar_id'
        ParamType = ptInput
        Value = nil
      end>
  end
