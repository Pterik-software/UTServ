  object UniSQLDismiss: TUniSQL
    Connection = FormDM.UniSQLite
    SQL.Strings = (
      'update users '
      'set is_active = :p_is_active, '
      'closure_date = '
      'case when :p_is_active = 0 then :p_closure_date '
      'else NULL end'
      'where user_id = :p_user_id')
    Left = 190
    Top = 288
    ParamData = <
